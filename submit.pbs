#!/bin/bash
#PBS -l nodes=1:ppn=16:dc2,walltime=0:20:00
#PBS -N app-autoalignacpc
#PBS -V

cd $PBS_O_WORKDIR

module load matlab
module load spm

export MATLABPATH=$MATLABPATH:$SERVICE_DIR
matlab -nodisplay -nosplash -r main

#check for output files
if [ -s t1.nii.gz ];
then
	echo 0 > finished
else
	echo "output t1.nii.gz missing"
	echo 1 > finished
	exit 1
fi

echo $? > finished
